{
    "name": "allinpay/yunshangtong",
    "description": "通联支付云商通 - 基于用户、商家、订单角色的业务系统",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["allinpay", "payment", "sdk", "php", "china", "financial", "yunshangtong"],
    "homepage": "https://github.com/CCoy521/allinpay-yunshangtong",
    "license": "MIT",
    "authors": [
        {
            "name": "Developer",
            "email": "developer@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "sop/crypto-types": "^0.3.0",
        "phpseclib/phpseclib": "~3.0",
        "mdanter/ecc": "^0.5.2",
        "lpilp/guomi": "^1.0.2",
        "ext-openssl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "latzou/sm2": "^2.0",
        "fgrosse/phpasn1": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AllinpayYunshangtong\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AllinpayYunshangtong\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
